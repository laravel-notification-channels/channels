{
    "name": "laravel-notification-channels/wxwork",
    "description": "WXWwork Noficaiton Channel for Laravel",
    "keywords": [
        "wxwork notification",
        "laravel",
        "wxwork",
        "notification",
        "wxwork notifications channel"
    ],
    "homepage": "https://github.com/laravel-notification-channels/wxwork",
    "license": "MIT",
    "authors": [
        {
            "name": "Dong Lei",
            "email": "dlsimple@gmail.com",
            "homepage": "https://www.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^8.77",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "^8.77"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\WXWork\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\WXWork\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
