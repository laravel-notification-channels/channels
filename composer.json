{
    "name": "laravel-notification-channels/pushover-notifications",
    "description": "Pushover notifications for Laravel.",
    "homepage": "https://github.com/laravel-notification-channels/pushover-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Boone",
            "email": "mail@casperboone.nl",
            "homepage": "https://casperboone.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "illuminate/events": "^5.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PushoverNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PushoverNotifications\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
