{
    "name": "laravel-notification-channels/asterisk",
    "description": "Asterisk Notifications Driver",
    "homepage": "https://github.com/laravel-notification-channels/asterisk",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Razumov (Enniel)",
            "email": "razumov.evgeni@gmail.com",
            "homepage": "https://github.com/enniel"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "enniel/ami": "1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Asterisk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Asterisk\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
