{
    "name": "laravel-notification-channels/notify",
    "description": "Notify Notifications driver for Laravel",
    "keywords": [
        "laravel",
        "notify",
        "notification",
        "notify notifications channel"
    ],
    "homepage": "https://github.com/laravel-notification-channels/notify",
    "license": "MIT",
    "authors": [
        {
            "name": "Notify",
            "email": "info@notify.eu",
            "homepage": "https://notify.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "illuminate/events": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Notify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Notify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Notify\\NotifyServiceProvider"
            ]
        }
    }
}
