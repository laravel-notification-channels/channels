{
    "name": "laravel-notification-channels/pubnub",
    "description": "PubNub Notifications Channel for Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/pubnub",
    "license": "MIT",
    "authors": [
        {
            "name": "iWader",
            "email": "wade@iwader.co.uk",
            "homepage": "https://iwader.co.uk/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/events": "^5.3@dev",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "pubnub/pubnub": "^3.8"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
