{
    "name": "laravel-notification-channels/hangouts-chat",
    "description": "Notifications from Laravel to Google Hangouts Chat",
    "homepage": "https://github.com/renanwilliam/hangouts-chat",
    "license": "MIT",
    "authors": [
        {
            "name": "Renan William Alves de Paula",
            "email": "renan@4success.com.br"
        }
    ],
    "require": {
        "php": ">=7.2",
        "google/apiclient": "^2.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\HangoutsChatChannelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
