{
    "name": "laravel-notification-channels/pagerduty",
    "description": "A Laravel notification channel for sending PagerDuty events.",
    "homepage": "https://github.com/laravel-notification-channels/pagerduty",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Waite",
            "email": "lwaite@gmail.com",
            "homepage": "https://lukewaite.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
