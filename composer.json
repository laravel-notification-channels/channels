{
    "name": "laravel-notification-channels/moceanapi",
    "description": "MoceanAPI notifications channel for Laravel 5.5+",
    "homepage": "https://github.com/laravel-notification-channels/moceanapi",
    "license": "MIT",
    "authors": [
        {
            "name": "NeoSon",
            "email": "lkloon123@hotmail.com",
            "homepage": "https://github.com/lkloon123",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "mocean/laravel-mocean": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\MoceanApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\MoceanApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\MoceanApi\\MoceanApiServiceProvider"
            ]
        }
    }
}
