{
    "name": "laravel-notification-channels/unifonic",
    "description": "Unifonic SMS notification channel for Laravel 5 and up",
    "homepage": "https://github.com/laravel-notification-channels/unifonic",
    "license": "MIT",
    "authors": [
        {
            "name": "imad",
            "email": "imad@devinweb.com",
            "homepage": "https://devinweb.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "illuminate/notifications": "~5.8 || ~6.0 || ^7.0 || ^8.0",
        "illuminate/support": "~5.8 || ~6.0 || ^7.0 || ^8.0",
        "orchestra/testbench": "~3.8.0 || ^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Unifonic\\UnifonicServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Unifonic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Unifonic\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
