{
    "name": "laravel-notification-channels/realtime-push-notifications",
    "description": "Realtime Push Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/realtime-push-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Realtime.co",
            "email": "framework@realtime.co",
            "homepage": "https://framework.realtime.co",
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\RealtimePushNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\RealtimePushNotifications\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
