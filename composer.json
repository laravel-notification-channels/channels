{
  "name": "laravel-notification-channels/redsms-ru",
  "description": "RedsmsRu Notifications channel for Laravel 5.3.",
  "keywords": [
    "redsms",
    "laravel",
    "notifications"
  ],
  "homepage": "https://github.com/laravel-notification-channels/redsms-ru",
  "license": "MIT",
  "authors": [
    {
      "name": "projct1",
      "role": "Developer",
      "email": "kindly1987@gmail.com",
      "homepage": "https://github.com/projct1"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\RedsmsRu\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\RedsmsRu\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\RedsmsRu\\RedsmsRuServiceProvider"
      ]
    }
  }
}
