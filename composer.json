{
    "name": "laravel-notification-channels/netgsm",
    "description": "NetGSM notification channel for Laravel 5.5 & 6.x"
    "homepage": "https://github.com/laravel-notification-channels/netgsm",
    "license": "MIT",
    "authors": [
        {
            "name": "Talha Zekeriya Durmu≈ü",
            "email": "drtzack@gmail.com",
            "homepage": "https://talhadurmus.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\NetGsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\NetGsm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\NetGsm\\NetGsmServiceProvider"
            ]
        }
    }
}
